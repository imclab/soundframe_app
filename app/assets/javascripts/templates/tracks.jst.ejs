<% for (var i in tracks){ %>
<div id="<%= tracks[i].id %>" class="small-12 columns">
  <div class="progress [small-# large-#] [secondary alert success] [radius round]">
  <span class="meter" style="width: [1 - 100]%"></span>
</div>
  <iframe width="100%" height="100" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=http%3A%2F%2Fapi.soundcloud.com%2Ftracks%2F<%= tracks[i].id %>&show_artwork=true"></iframe>
  <a class="save_track" data-id="<%= tracks[i].id %>">Save Track</a>
</div>
<%}%>
